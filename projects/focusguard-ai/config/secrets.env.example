# FocusGuard AI - Environment Variables Template
# Copy this file to secrets.env and fill in your actual values
# NEVER commit secrets.env to git

# Twilio (Phone Integration)
TWILIO_ACCOUNT_SID=your_account_sid_here
TWILIO_AUTH_TOKEN=your_auth_token_here
TWILIO_PHONE_NUMBER=+1234567890

# Voice AI Provider (Choose one)
# Bland.ai (Recommended - $0.04/min)
BLAND_API_KEY=your_bland_api_key_here

# IsOn24 (Alternative - $0.049/min)
ISON24_API_KEY=your_ison24_api_key_here

# Database (if using PostgreSQL instead of SQLite)
DATABASE_URL=postgresql://user:password@localhost/focusguard

# Security
SECRET_KEY=your_secret_key_for_jwt_tokens

# Environment
ENVIRONMENT=development
DEBUG=true
